@{
    ViewBag.Title = "Home Page";
    List<SelectListItem> items = (List<SelectListItem>)ViewBag.items;
}



<div class="row">
    <div class="col-md-6">

    </div>
</div>
<br />
<br />

<div class="jumbotron  ">
    <div class="row">
        <div class="col-6 ">
            <form action="/Home/create" method="post" id="frmVehiculo">

                <div class="form-group row ">
                    <h3>Vehículo</h3>

                </div>

                <div class="form-group row ">
                    <label for="placa" class="col-sm-1 col-form-label">Placa</label>
                    <input id="placa" type="text" class="form-control" name="placa" />

                </div>


                <div class="form-group row">
                    <label for="dueño" class="col-sm-1 col-form-label">Dueño</label>
                    <input id="dueño" type="text" class="form-control" name="dueño" />
                </div>

                <div class="form-group row">
                    <label for="marca" class="col-sm-1 col-form-label">Marca</label>
                    <input id="marca" type="text" class="form-control" name="marca" />


                </div>

                <div class="form-group row">

                    <label class="col-sm-1 col-form-label" for="cars">Servicio</label>

                    @Html.DropDownList("datos", items, "Seleccionar", new { @class = "form-control" })

                </div>


                <br />
                <div>
                    <input type="submit" class="btn btn-success "
                           value="Registrar" />
                </div>
            </form>
        </div>




    </div>
</div>
<br />